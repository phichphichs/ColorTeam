<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>List</servlet-name>
        <servlet-class>Controller.List</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Detail_Member</servlet-name>
        <servlet-class>Controller.Detail_Member</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact_Member</servlet-name>
        <servlet-class>Controller.Contact_Member</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertMember</servlet-name>
        <servlet-class>Controller.InsertMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMember</servlet-name>
        <servlet-class>Controller.UpdateMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMember</servlet-name>
        <servlet-class>Controller.DeleteMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListEvent</servlet-name>
        <servlet-class>Controller.ListEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEvent</servlet-name>
        <servlet-class>Controller.InsertEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateEvent</servlet-name>
        <servlet-class>Controller.UpdateEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteEvent</servlet-name>
        <servlet-class>Controller.DeleteEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controller.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>List</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Detail_Member</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact_Member</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertMember</servlet-name>
        <url-pattern>/insertMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMember</servlet-name>
        <url-pattern>/updateMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMember</servlet-name>
        <url-pattern>/deleteMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListEvent</servlet-name>
        <url-pattern>/listEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEvent</servlet-name>
        <url-pattern>/insertEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateEvent</servlet-name>
        <url-pattern>/updateEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteEvent</servlet-name>
        <url-pattern>/deleteEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            100
        </session-timeout>
    </session-config>
</web-app>
